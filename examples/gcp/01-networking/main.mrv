#lang marv

import "network-base.mrv" as net

module main(name) {

     defaults = {
          # env is a built-in function to read environment variables
          project = env("MARV_GCP_PROJECT")
          region = env("MARV_GCP_REGION")
     }

     network = net/network-base(defaults = defaults, name = name)
     return { xxx = 123 }
}