#lang marv

import types/gcp/storage as gcp

module main {

    defaults = {
        project = env("MARV_GCP_PROJECT")
        region = env("MARV_GCP_REGION")
    }

    bucket = gcp:bucket defaults <- {
        name = strf("~a-mybucket" defaults.project)
        # storageClass = "NEARLINE"
    }
    bucket2 = gcp:nearlineb defaults <- {
        name = strf("~a-mybucket" defaults.project)
        # storageClass = "NEARLINE"
    }
}
